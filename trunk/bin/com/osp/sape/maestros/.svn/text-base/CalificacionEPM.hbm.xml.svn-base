<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="com.osp.sape.maestros.CalificacionEPM"
        table="calificacion_oss_epm"
    >

        <id
            name="telefono"
            column="telefono"
            type="java.lang.Long"
            unsaved-value="-1"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-CalificacionEPM.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="identificador"
            type="string"
            update="true"
            insert="true"
            column="identificador"
        />

        <property
            name="idCentral"
            type="string"
            update="true"
            insert="true"
            column="id_central"
        />

        <property
            name="red"
            type="string"
            update="true"
            insert="true"
            column="red"
        />

        <property
            name="stripCaja"
            type="string"
            update="true"
            insert="true"
            column="strip_caja"
        />

        <property
            name="par"
            type="string"
            update="true"
            insert="true"
            column="par"
        />

        <property
            name="calificacionDatos"
            type="string"
            update="true"
            insert="true"
            column="calificacion_datos"
        />

        <property
            name="estadoLinea"
            type="string"
            update="true"
            insert="true"
            column="estado_linea"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CalificacionEPM.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
