        name="com.osp.sape.maestros.SiplexPROAutotest"
        table="siplexpro_autotest"
    >

        <id
            name="id"
            column="id"
            type="int"
            unsaved-value="-1"
        >
            <generator class="sequence">
                <param name="sequence">idsiplexpro_autotest</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SiplexPROAutotest.xml 
                  containing the additional parameters and place it in your merge dir. 
