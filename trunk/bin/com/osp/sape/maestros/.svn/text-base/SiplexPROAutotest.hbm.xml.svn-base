            type="string"
            update="true"
            insert="true"
            column="ethernet"
        />

        <property
            name="modem"
            type="string"
            update="true"
            insert="true"
            column="modem"
        />

        <property
            name="flash_mem"
            type="string"
            update="true"
            insert="true"
            column="flash_mem"
        />

        <property
            name="tarjeta_expansion"
            type="string"
            update="true"
            insert="true"
            column="tarjeta_expansion"
        />

        <property
            name="controlador_troncal"
            type="string"
            update="true"
            insert="true"
            column="controlador_troncal"
        />

        <property
            name="observacion"
            type="string"
            update="true"
            insert="true"
            column="observacion"
        />

        <property
            name="ultimoUsuario"
            type="string"
            update="true"
            insert="true"
            column="ultimousuario"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SiplexPROAutotest.xml
