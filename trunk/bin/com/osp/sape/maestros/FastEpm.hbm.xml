<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="com.osp.sape.maestros.FastEpm"
        table="fast_eeppm"
    >

        <id
            name="telefonoFast"
            column="numero_telefono"
            type="java.lang.String"
            unsaved-value="-1"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-FastEpm.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="id"
            type="integer"
            update="true"
            insert="true"
            column="idtiponodo"
        />

        <property
            name="numeroFast"
            type="integer"
            update="true"
            insert="true"
            column="numerofast"
        />

        <property
            name="id2"
            type="integer"
            update="true"
            insert="true"
            column="indigo_secundario"
        />

        <property
            name="ll"
            type="string"
            update="true"
            insert="true"
            column="ll"
        />

        <property
            name="vertical"
            type="string"
            update="true"
            insert="true"
            column="vertical"
        />

        <property
            name="central"
            type="string"
            update="true"
            insert="true"
            column="central"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-FastEpm.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
