package com.osp.sape.Exceptions;

/**
 * Clase que representarï¿½ todas las excepciones que tiene que ver con la aplicacion.
 * @author Andres Aristizabal
 */
public class SapeAppException extends Exception {


	private static final long serialVersionUID = 1L;

	public SapeAppException() {
        super();
    }


    public SapeAppException(String message) {
        super(message);
    }


    public SapeAppException(Throwable cause) {
        super(cause);
    }

    public SapeAppException(String message, Throwable cause) {
        super(message, cause);
    }

}
