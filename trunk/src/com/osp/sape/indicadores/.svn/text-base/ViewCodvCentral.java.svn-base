package com.osp.sape.indicadores;

import java.sql.Date;

public class ViewCodvCentral {
	private Date fecha;
	private String central;
	private String site;
	private String codigoVer;
	private int cantidad;

	public ViewCodvCentral(String central, String site, String codigoVer, Date fecha, int cantidad) {
		this.cantidad = cantidad;
		this.central = central;
		this.codigoVer = codigoVer;
		this.fecha = fecha;
		this.site = site;
	}

	public int getCantidad() {
		return cantidad;
	}

	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}

	public String getCentral() {
		return central;
	}

	public void setCentral(String central) {
		this.central = central;
	}

	public String getCodigoVer() {
		return codigoVer;
	}

	public void setCodigoVer(String codigoVer) {
		this.codigoVer = codigoVer;
	}

	public Date getFecha() {
		return fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}

	public String getSite() {
		return site;
	}

	public void setSite(String site) {
		this.site = site;
	}
	
	
	
}
