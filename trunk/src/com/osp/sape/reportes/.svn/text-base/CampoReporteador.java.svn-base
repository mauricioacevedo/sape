package com.osp.sape.reportes;

/**
 * Clase que representa un campo a escoger dentro de la pagina del reporteador 
 * @author Andres Aristizabal
 *
 */
public class CampoReporteador {

	/** Nombre del campo de la BD **/
	private String campo;
	
	/** Nomrbe Original a mostrar en la Tabla **/
	private String nombre;
//	private String etiqueta;
	
	/** Si se permite o no filtrar por este campo **/
	private boolean filtro;
	
	
	
	public CampoReporteador(String campo, String nombre, boolean filtro) {
		this.campo = campo;
		this.nombre = nombre;
		this.filtro = filtro;
	}

	public String getCampo() {
		return campo;
	}
	
	public void setCampo(String campo) {
		this.campo = campo;
	}
	
	public boolean isFiltro() {
		return filtro;
	}
	
	public void setFiltro(boolean filtro) {
		this.filtro = filtro;
	}
	
	public String getNombre() {
		return nombre;
	}
	
	public void setNombre(String nombre) {
		this.nombre = nombre;
	} 
	
	
	
}

