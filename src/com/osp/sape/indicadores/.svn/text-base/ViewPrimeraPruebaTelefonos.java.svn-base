package com.osp.sape.indicadores;

import java.util.Date;

public class ViewPrimeraPruebaTelefonos {

	private int idPrueba;
	private String telefono;
	private int cantidad;
	private String tipoPrueba;
	private String codigoVer;
	private String tipoNodo;
	private String cliente;
	private String estado;
	private Date fechaInicial;
	private String duracion;
	
	
	
	public ViewPrimeraPruebaTelefonos(int prueba, String telefono, int cantidad, String tipoPrueba, String codV, String tipoNodo, String cliente, String estado, Date fechaInicial, String duracion) {
		idPrueba = prueba;
		this.telefono = telefono;
		this.cantidad = cantidad;
		this.tipoPrueba = tipoPrueba;
		codigoVer = codV;
		this.tipoNodo = tipoNodo;
		this.cliente = cliente;
		this.estado = estado;
		this.fechaInicial = fechaInicial;
		this.duracion = duracion;
	}
	
	public int getCantidad() {
		return cantidad;
	}
	
	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}
	public String getCliente() {
		return cliente;
	}
	public void setCliente(String cliente) {
		this.cliente = cliente;
	}
	public String getCodigoVer() {
		return codigoVer;
	}
	public void setCodigoVer(String codigoVer) {
		this.codigoVer = codigoVer;
	}
	public String getDuracion() {
		return duracion;
	}
	public void setDuracion(String duracion) {
		this.duracion = duracion;
	}
	public String getEstado() {
		return estado;
	}
	public void setEstado(String estado) {
		this.estado = estado;
	}
	
	public Date getFechaInicial() {
		return fechaInicial;
	}
	public void setFechaInicial(Date fechaInicial) {
		this.fechaInicial = fechaInicial;
	}
	public int getIdPrueba() {
		return idPrueba;
	}
	public void setIdPrueba(int idPrueba) {
		this.idPrueba = idPrueba;
	}
	public String getTelefono() {
		return telefono;
	}
	public void setTelefono(String telefono) {
		this.telefono = telefono;
	}
	public String getTipoNodo() {
		return tipoNodo;
	}
	public void setTipoNodo(String tipoNodo) {
		this.tipoNodo = tipoNodo;
	}
	public String getTipoPrueba() {
		return tipoPrueba;
	}
	public void setTipoPrueba(String tipoPrueba) {
		this.tipoPrueba = tipoPrueba;
	}
	
	protected String paramString() {
		return 	"idPrueba=" + idPrueba + ";telefono=" + telefono + ";cantidad=" + cantidad + 
			";tipoPrueba=" + tipoPrueba + ";codigoVer=" + codigoVer + ";tipoNodo=" + tipoNodo + 
			";cliente=" + cliente + ";estado=" + estado + ";fechaInicial=" + fechaInicial +
			";duracion=" + duracion;
	}
	
	
	public String toString() {
		return getClass().getName() + ": [" + paramString() + "]";
	}
	
}
