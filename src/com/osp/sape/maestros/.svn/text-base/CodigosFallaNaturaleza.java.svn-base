package com.osp.sape.maestros;

public abstract class CodigosFallaNaturaleza {
	
	protected String fallaId;
	protected String nombre;
		
	public abstract String getFallaId();
	
	public abstract void setFallaId(String fallaId);
	
	public abstract String getNombre();
	
	public abstract void setNombre(String nombre);
	
    protected String paramString() {
    	return "fallaId=" + fallaId + "; nombre=" + nombre;
    }
    
    public String toString() {
    	return getClass().getName() + "[" + paramString() + "]";
    }
}
